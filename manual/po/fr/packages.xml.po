#| "\n"
#| "LH_PACKAGES_LISTS=\"gnome\"\n"
msgid "LH_PACKAGES_LISTS=\"gnome\""
msgstr ""
"\n"
"LH_PACKAGES_LISTS=\"gnome\"\n"

# type: Content of: <sect1><sect2><sect3><para>
