#. type: Content of: <sect1><sect2><sect3><para><screen>
#: en/chapters/customization/packages.xml:222
#, fuzzy, no-wrap
#| msgid ""
#| "\n"
#| "LH_PACKAGES=\"package1 package2 package3 ... \"\n"
msgid "LH_PACKAGES=\"package1 package2 package3 ... \""
msgstr ""
